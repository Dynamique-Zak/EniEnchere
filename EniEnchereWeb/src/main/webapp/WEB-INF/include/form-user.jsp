<div>
<!-- Input : Pseudo -->
  <div class="mb-3">
      <label for="eech-pseudo" class="form-label">Pseudo</label>
      <input type="text" id="eech-pseudo" name="pseudo" class="form-control" value="${user.pseudo}">
  </div>
  <!-- ./Input : Pseudo -->

  <!-- Input : Nom -->
  <div class="mb-3">
      <label for="eech-nom" class="form-label">Nom</label>
      <input type="text" id="eech-nom" name="name" class="form-control" value="${user.nom}">
  </div>
  <!-- ./Input : Nom -->

  <!-- Input : Prenom -->
  <div class="mb-3">
      <label for="" class="form-label">Prenom</label>
      <input type="text" name="prenom" class="form-control" value="${user.prenom}">
  </div>
  <!-- ./Input : Prenom -->

  <!-- Input : Email -->
  <div class="mb-3">
      <label for="" class="form-label">Email</label>
      <input type="text" name="email" class="form-control" value="${user.email}">
  </div>
  <!-- ./Input : Email -->

  <!-- Input : Telephone -->
  <div class="mb-3">
      <label for="" class="form-label">Telephone</label>
      <input type="text" name="phone" class="form-control" value="${user.telephone}">
  </div>
  <!-- ./Input : Telephone -->

  <!-- Input : Rue -->
  <div class="mb-3">
      <label for="" class="form-label">Rue</label>
      <input type="text" name="rue" class="form-control" value="${user.rue}">
  </div>
